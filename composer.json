{
    "name": "zendframework/zendamf",
    "description": "provides support for Adobe's » Action Message Format (AMF), to allow communication between Adobe's » Flash Player and PHP",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "amf"
    ],
    "homepage": "http://packages.zendframework.com/",
    "autoload": {
        "psr-0": {
            "ZendAmf": "library/",
            "ZendAmfTest\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-authentication": ">=2.0.0",
        "zendframework/zend-db": ">=2.0.0",
        "zendframework/zend-loader": ">=2.0.0",
        "zendframework/zend-permissions-acl": ">=2.0.0",
        "zendframework/zend-server": ">=2.0.0"
    },
    "require-dev": {
        "zendframework/zendframework": "dev-master"
    }
}
